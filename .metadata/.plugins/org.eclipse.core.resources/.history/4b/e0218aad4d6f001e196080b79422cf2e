package com.example.hibernatemapping.repository.OneToOne;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.example.hibernatemapping.entity.EmployeeOTOMapping;

@Repository
public interface EmployeeRepositoryOTOMapping extends JpaRepository<EmployeeOTOMapping, Integer> {
	
	List<EmployeeOTOMapping> findByName(String name);

}
