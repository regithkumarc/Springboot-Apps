package com.example.hibernatemapping.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.example.hibernatemapping.entity.AuthorManyToMany;

@Repository
public interface EmployeeRepositoryManyToMany extends JpaRepository<AuthorManyToMany, Integer> {
	
	List<AuthorManyToMany> findByName(String name);

}
